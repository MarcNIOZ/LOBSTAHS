#   pages="??--??",
#   url="http://??",
   textVersion = paste("Collins, J. R., B. R. Edwards, H. F. Fredricks, and B. A. S. Van Mooy",
                       "Untargeted discovery and identification of oxidative stress biomarkers using LOBSTAHS, a lipidomics pipeline for complex datasets")